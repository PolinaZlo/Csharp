<Window x:Class="Lab3D.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab3D"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="vbBtn" Content="VisualButton" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="93" Height="38" Click="VisualButton_Click"/>
        <Button x:Name="grBtn" Content="Gradient" HorizontalAlignment="Left" Margin="10,71,0,0" VerticalAlignment="Top" Width="93" Height="38" Click="GrBtn_Click"/>
        <Button x:Name="radBtn" Content="Radial" HorizontalAlignment="Left" Margin="10,135,0,0" VerticalAlignment="Top" Width="93" Height="38" Click="radBtn_Click"/>
        <Button x:Name="imgBtn" Content="Image" HorizontalAlignment="Left" Margin="10,202,0,0" VerticalAlignment="Top" Width="93" Height="38" Click="ImgBtn_Click"/>
        <Calendar x:Name="Trolling" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="476,135,0,0"/>
        <Ellipse x:Name="el" Fill="White" HorizontalAlignment="Left" Height="319" Margin="377,49,0,0" Stroke="Black" VerticalAlignment="Top" Width="363"/>
    </Grid>
</Window>
